<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Device_token</th>
	    <th>updated</th>
	    <th>created</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
	      <td><%= user.device_token %></td>

	      <td><%= user.updated_at.strftime('%H:%M:%S') %></td>
	      <td><%= user.created_at.strftime('%H:%M:%S') %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
